<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="101dp"
        android:layout_height="23dp"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="22dp"
        android:text="Voiture Model :"
        app:layout_constraintBottom_toTopOf="@+id/textView20"
        app:layout_constraintEnd_toStartOf="@+id/modelCar"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="101dp"
        android:layout_height="23dp"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="57dp"
        android:text="Trajet"
        app:layout_constraintBottom_toTopOf="@+id/textView17"
        app:layout_constraintEnd_toStartOf="@+id/nameTrajet"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="101dp"
        android:layout_height="23dp"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="57dp"
        android:text="Voiture NicName :"
        app:layout_constraintBottom_toTopOf="@+id/textView22"
        app:layout_constraintEnd_toStartOf="@+id/nickNameCar"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="101dp"
        android:layout_height="23dp"
        android:layout_marginStart="72dp"
        android:layout_marginEnd="57dp"
        android:layout_marginBottom="35dp"
        android:text="Distance (km)"
        app:layout_constraintBottom_toTopOf="@+id/textView23"
        app:layout_constraintEnd_toStartOf="@+id/distance"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="101dp"
        android:layout_height="23dp"
        android:layout_marginStart="73dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="38dp"
        android:text="Dénivelé +"
        app:layout_constraintBottom_toTopOf="@+id/textView24"
        app:layout_constraintEnd_toStartOf="@+id/upTrip"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="101dp"
        android:layout_height="23dp"
        android:layout_marginStart="73dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="34dp"
        android:text="Dénivelé -"
        app:layout_constraintBottom_toTopOf="@+id/textView25"
        app:layout_constraintEnd_toStartOf="@+id/downTrip"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView25"
        android:layout_width="123dp"
        android:layout_height="36dp"
        android:layout_marginStart="75dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="33dp"
        android:text="Consommation électricité (KW)"
        app:layout_constraintBottom_toTopOf="@+id/textView26"
        app:layout_constraintEnd_toStartOf="@+id/consoElect"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="123dp"
        android:layout_height="36dp"
        android:layout_marginStart="75dp"
        android:layout_marginEnd="32dp"
        android:text="Consommation carurant (L)"
        app:layout_constraintEnd_toStartOf="@+id/consoFuel"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="531dp" />

    <TextView
        android:id="@+id/nameTrajet"
        android:layout_width="132dp"
        android:layout_height="28dp"
        android:layout_marginBottom="52dp"
        android:text="anonymous"
        app:layout_constraintBottom_toTopOf="@+id/modelCar"
        tools:layout_editor_absoluteX="230dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/modelCar"
        android:layout_width="132dp"
        android:layout_height="28dp"
        android:layout_marginBottom="17dp"
        android:text="anonymous"
        app:layout_constraintBottom_toTopOf="@+id/nickNameCar"
        tools:layout_editor_absoluteX="230dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/nickNameCar"
        android:layout_width="132dp"
        android:layout_height="28dp"
        android:layout_marginBottom="52dp"
        android:text="inconnu"
        app:layout_constraintBottom_toTopOf="@+id/distance"
        tools:layout_editor_absoluteX="230dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/distance"
        android:layout_width="132dp"
        android:layout_height="28dp"
        android:layout_marginBottom="30dp"
        android:text="inconnu"
        app:layout_constraintBottom_toTopOf="@+id/upTrip"
        tools:layout_editor_absoluteX="230dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/downTrip"
        android:layout_width="132dp"
        android:layout_height="28dp"
        android:layout_marginBottom="37dp"
        android:text="inconnu"
        app:layout_constraintBottom_toTopOf="@+id/consoElect"
        tools:layout_editor_absoluteX="230dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/upTrip"
        android:layout_width="132dp"
        android:layout_height="28dp"
        android:layout_marginBottom="28dp"
        android:text="inconnu"
        app:layout_constraintBottom_toTopOf="@+id/downTrip"
        tools:layout_editor_absoluteX="230dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/consoElect"
        android:layout_width="132dp"
        android:layout_height="28dp"
        android:layout_marginBottom="41dp"
        android:text="inconnu"
        app:layout_constraintBottom_toTopOf="@+id/consoFuel"
        tools:layout_editor_absoluteX="230dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/consoFuel"
        android:layout_width="132dp"
        android:layout_height="28dp"
        android:text="inconnu"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="539dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="visible"
    tools:visibility="visible">

    <com.example.ourapplication_kohl_roux_m.ConsumptionsCapture
        android:id="@+id/consumptionsCapture2"
        android:layout_width="150dp"
        android:layout_height="361dp"
        android:layout_gravity="right"
        android:layout_marginTop="150dp"
        android:background="@android:drawable/ic_input_add"
        android:paddingLeft="80dp"
        android:paddingBottom="40dp"
        android:visibility="visible"
        app:exampleColor="#33b5e5"
        app:exampleDimension="24sp"
        app:exampleString="Hello, ConsumptionsCapture"
        tools:visibility="visible" />

    <com.example.ourapplication_kohl_roux_m.ConsumptionsCapture
        android:id="@+id/ListChargeElectricQ"
        android:layout_width="150dp"
        android:layout_height="361dp"
        android:layout_marginTop="150dp"
        android:background="#ccc"
        android:fadeScrollbars="true"
        android:fadingEdge="horizontal"
        android:paddingLeft="80dp"
        android:paddingBottom="40dp"
        android:visibility="visible"
        app:exampleColor="#1DCDBD"
        app:exampleDimension="24sp"
        app:exampleDrawable="@android:drawable/ic_menu_add"
        app:exampleString="Hello, ConsumptionsCapture"
        tools:visibility="visible" />

    <Button
        android:id="@+id/Add_PwrCosumpt"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginLeft="155dp"
        android:layout_marginTop="100dp"
        android:background="@android:drawable/ic_input_add"
        android:onClick="addEnergyConsumpt" />

    <Button
        android:id="@+id/button3"
        android:layout_width="32dp"
        android:layout_height="31dp"
        android:layout_gravity="right|center_vertical"
        android:layout_marginEnd="20dp"
        android:background="@android:drawable/ic_delete"
        android:visibility="visible"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="243dp"
        tools:visibility="visible" />

    <Button
        android:id="@+id/button4"
        android:layout_width="32dp"
        android:layout_height="31dp"
        android:layout_gravity="clip_horizontal|center|clip_vertical|center_horizontal|fill_horizontal"
        android:layout_marginStart="80dp"
        android:background="@android:drawable/ic_delete"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="@id/ListChargeElectricQ"
        app:layout_constraintStart_toStartOf="@id/ListChargeElectricQ"
        app:layout_constraintTop_toTopOf="@id/ListChargeElectricQ"
        tools:visibility="visible" />

    <EditText
        android:id="@+id/SaisieElec"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="100dp"
        android:inputType="numberDecimal"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:visibility="visible"
        tools:visibility="visible" />

    <EditText
        android:id="@+id/SaisieCarb"
        android:layout_width="120dp"
        android:layout_height="40dp"
        android:layout_marginLeft="210dp"
        android:layout_marginTop="100dp"
        android:inputType="numberDecimal"
        android:visibility="visible"
        tools:visibility="visible" />


</FrameLayout>
